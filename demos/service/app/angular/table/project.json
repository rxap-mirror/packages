{
  "name": "service-app-angular-table",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "demos/service/app/angular/table/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/demos/service/app/angular/table",
        "main": "demos/service/app/angular/table/src/main.ts",
        "tsConfig": "demos/service/app/angular/table/tsconfig.app.json",
        "assets": [
          "demos/service/app/angular/table/src/assets"
        ],
        "isolatedConfig": true,
        "webpackConfig": "demos/service/app/angular/table/webpack.config.js",
        "generatePackageJson": true
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "demos/service/app/angular/table/src/environments/environment.ts",
              "with": "demos/service/app/angular/table/src/environments/environment.prod.ts"
            }
          ]
        },
        "development": {
          "progress": true
        }
      }
    },
    "docker": {
      "options": {
        "imageSuffix": "/service/app-angular-table",
        "dockerfile": "shared/nestjs/Dockerfile",
        "buildArgList": [
          "PATH_PREFIX=REGEX:app/app.config.ts:validationSchema\\['GLOBAL_API_PREFIX'\\] = Joi.string\\(\\).default\\('(.+)'\\);"
        ]
      }
    },
    "docker-save": {},
    "generate-open-api": {},
    "generate-package-json": {},
    "lint": {
      "options": {
        "lintFilePatterns": [
          "demos/service/app/angular/table/**/*.ts"
        ]
      }
    },
    "save": {
      "executor": "@rxap/plugin-docker:save"
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "service-app-angular-table:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "service-app-angular-table:build:development"
        },
        "production": {
          "buildTarget": "service-app-angular-table:build:production"
        }
      }
    },
    "swagger-build": {
      "options": {
        "outputPath": "dist/swagger/demos/service/app/angular/table",
        "main": "demos/service/app/angular/table/src/swagger.ts",
        "webpackConfig": "demos/service/app/angular/table/webpack.config.js",
        "transformers": [
          "@nestjs/swagger/plugin"
        ],
        "tsConfig": "demos/service/app/angular/table/tsconfig.app.json",
        "fileReplacements": [
          {
            "replace": "demos/service/app/angular/table/src/environments/environment.ts",
            "with": "demos/service/app/angular/table/src/environments/environment.swagger.ts"
          }
        ]
      }
    },
    "swagger-generate": {
      "outputs": [
        "dist/swagger/{projectRoot}/openapi.json"
      ],
      "options": {
        "buildTarget": "service-app-angular-table:swagger-build"
      }
    },
    "test": {
      "options": {
        "jestConfig": "demos/service/app/angular/table/jest.config.ts"
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": [
    "service",
    "nest",
    "sentry",
    "open-api",
    "backend",
    "swagger",
    "health-indicator",
    "express"
  ]
}
