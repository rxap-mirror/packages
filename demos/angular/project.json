{
  "name": "angular",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "rxap",
  "sourceRoot": "demos/angular/src",
  "implicitDependencies": [
    "angular-authentication",
    "angular-authorization",
    "angular-components",
    "angular-data-grid",
    "angular-dialog",
    "angular-environment",
    "angular-layout",
    "angular-oauth",
    "angular-service-worker",
    "angular-table-system",
    "angular-tree",
    "angular-upload",
    "angular-window-system",
    "angular-material-form-system",
    "angular-material-table-system",
    "angular-data-source",
    "angular",
    "angular-material-table-select"
  ],
  "tags": [
    "angular",
    "ngx",
    "frontend"
  ],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/demos/angular",
        "index": "demos/angular/src/index.html",
        "main": "demos/angular/src/main.ts",
        "polyfills": [
          "zone.js",
          "@angular/localize/init"
        ],
        "tsConfig": "demos/angular/tsconfig.app.json",
        "assets": [
          "demos/angular/src/favicon.ico",
          "demos/angular/src/assets",
          {
            "glob": "*.json",
            "input": "config/",
            "output": "/"
          },
          {
            "glob": "*",
            "input": "shared/assets/",
            "output": "/"
          },
          "demos/angular/src/manifest.webmanifest",
          "demos/angular/src/build.json"
        ],
        "styles": [
          "demos/angular/src/styles.scss"
        ],
        "scripts": [],
        "sourceMap": true
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "10mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all",
          "serviceWorker": true,
          "ngswConfigPath": "shared/ngsw-config.json"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "angular:build:production"
        },
        "development": {
          "browserTarget": "angular:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "angular:build",
        "format": "xliff2",
        "outputPath": "demos/angular/src/i18n"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "demos/angular/**/*.ts",
          "demos/angular/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "demos/angular/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "component-test": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "demos/angular/cypress.config.ts",
        "testingType": "component",
        "skipServe": true,
        "devServerTarget": "angular:build:development"
      },
      "configurations": {
        "open": {
          "watch": true
        }
      }
    },
    "config": {
      "executor": "@rxap/plugin-application:config",
      "options": {},
      "configurations": {
        "production": {},
        "development": {}
      }
    },
    "build-info": {
      "executor": "@rxap/plugin-application:build-info",
      "options": {},
      "configurations": {
        "production": {},
        "development": {}
      }
    },
    "docker": {
      "executor": "@rxap/plugin-docker:build",
      "options": {},
      "configurations": {
        "production": {},
        "development": {}
      }
    },
    "docker-save": {
      "executor": "@rxap/plugin-docker:save",
      "options": {},
      "configurations": {
        "production": {},
        "development": {}
      }
    }
  }
}
