{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx-cloud",
      "options": {
        "parallel": 32,
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "compodoc",
          "typedoc",
          "component-test"
        ],
        "accessToken": "YjMxMDI0NDUtYmZmMC00MzE4LWEyZjAtNWFlZDM1ODc1MTQ1fHJlYWQtd3JpdGU="
      }
    }
  },
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*"
    ],
    "typescript": [
      "{projectRoot}/**/*.ts",
      "{projectRoot}/tsconfig.json",
      "{projectRoot}/tsconfig.*.json"
    ],
    "test": [
      "typescript",
      "{projectRoot}/src/**/*",
      "!{projectRoot}/tsconfig.lib.json",
      "!{projectRoot}/tsconfig.lib.prod.json"
    ],
    "production": [
      "typescript",
      "{projectRoot}/src/**/*",
      "!{projectRoot}/**/*.{spec,stories,cy}.ts",
      "!{projectRoot}/jest.config.ts",
      "!{projectRoot}/src/test-setup.[jt]s",
      "!{projectRoot}/tsconfig.spec.json"
    ]
  },
  "targetDefaults": {
    "lint": {
      "inputs": [
        "default",
        "{workspaceRoot}/.eslintrc.json",
        "{workspaceRoot}/.eslintignore"
      ]
    },
    "fix-dependencies": {
      "inputs": [
        "typescript"
      ]
    },
    "update-package-group": {
      "inputs": [
        "production"
      ]
    },
    "update-dependencies": {
      "inputs": [
        "{workspaceRoot}/package.json",
        "{projectRoot}/package.json"
      ]
    },
    "test": {
      "inputs": [
        "test",
        "^test",
        "{workspaceRoot}/jest.preset.js",
        "{workspaceRoot}/jest.preset.ts"
      ]
    },
    "build": {
      "dependsOn": [
        "^build",
        {
          "target": "readme",
          "projects": "self"
        },
        {
          "target": "index-export",
          "projects": "self"
        }
      ],
      "inputs": [
        "production",
        "{projectRoot}/package.json",
        "^production"
      ]
    },
    "readme": {
      "inputs": [
        "{projectRoot}/README.md.handlebars",
        "{projectRoot}/GETSTARTED.md",
        "{projectRoot}/GUIDES.md",
        "{projectRoot}/package.json",
        "{projectRoot}/collection.json",
        "{projectRoot}/generators.json",
        "{projectRoot}/executors.json",
        "{projectRoot}/builders.json"
      ],
      "outputs": [
        "{projectRoot}/README.md"
      ]
    }
  },
  "affected": {
    "defaultBase": "development"
  },
  "generators": {
    "@nx/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "none",
      "tags": "angular,ngx",
      "prefix": "rxap"
    },
    "@nx/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest",
      "publishable": true,
      "addTailwind": true,
      "changeDetection": "OnPush",
      "standalone": true,
      "style": "scss",
      "directory": "angular",
      "tags": "angular,ngx",
      "prefix": "rxap"
    },
    "@nx/plugin:plugin": {
      "directory": "plugin",
      "publishable": true,
      "tags": "plugin,nx,nx-plugin"
    },
    "@nx/js:library": {
      "unitTestRunner": "jest",
      "publishable": true
    },
    "@nx/angular:component": {
      "style": "scss"
    },
    "@nx/nest:library": {
      "directory": "nest",
      "tags": "nest",
      "publishable": true
    }
  },
  "workspaceLayout": {
    "appsDir": "demos",
    "libsDir": "packages"
  }
}
