{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "affected": {
    "defaultBase": "master"
  },
  "npmScope": "rxap",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "test",
          "lint",
          "e2e",
          "build-storybook",
          "build-migrations",
          "build-schematics",
          "readme",
          "update-package-group",
          "update-peer-dependencies"
        ],
        "parallel": 5,
        "accessToken": "NWQxMzM4ODktNzk2Zi00M2IyLWFiYTktOTU2ZWFhNjNkNjU0fHJlYWQtd3JpdGU="
      }
    }
  },
  "targetDefaults": {
    "build-schematics": {
      "dependsOn": [
        {
          "target": "build",
          "projects": "self"
        }
      ]
    },
    "build-migrations": {
      "dependsOn": [
        {
          "target": "build",
          "projects": "self"
        }
      ]
    },
    "version": {
      "dependsOn": [
        {
          "target": "build",
          "projects": "self"
        },
        {
          "target": "build-migrations",
          "projects": "self"
        },
        {
          "target": "build-schematics",
          "projects": "self"
        }
      ]
    },
    "build": {
      "dependsOn": [
        {
          "target": "build",
          "projects": "dependencies"
        },
        {
          "target": "readme",
          "projects": "self"
        },
        {
          "target": "update-package-group",
          "projects": "self"
        },
        {
          "target": "update-peer-dependencies",
          "projects": "self"
        }
      ]
    },
    "update-peer-dependencies": {
      "dependsOn": [
        {
          "target": "update-package-group",
          "projects": "self"
        }
      ]
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "packageManager": "yarn",
    "defaultCollection": "@rxap/schematics"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  }
}
