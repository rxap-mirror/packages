{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx-cloud",
      "options": {
        "parallel": 32,
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "compodoc",
          "typedoc",
          "component-test"
        ],
        "accessToken": "YjMxMDI0NDUtYmZmMC00MzE4LWEyZjAtNWFlZDM1ODc1MTQ1fHJlYWQtd3JpdGU="
      }
    }
  },
  "targetDefaults": {
    "lint": {
      "inputs": [
        "default",
        "{workspaceRoot}/.eslintrc.json",
        "{workspaceRoot}/.eslintignore"
      ]
    },
    "test": {
      "inputs": [
        "default",
        "^default",
        "{workspaceRoot}/jest.preset.js"
      ]
    },
    "build": {
      "dependsOn": [
        "^build",
        {
          "target": "readme",
          "projects": "self"
        },
        {
          "target": "scss-bundle",
          "projects": "self"
        },
        {
          "target": "index-export",
          "projects": "self"
        }
      ]
    },
    "readme": {
      "inputs": [
        "{projectRoot}/README.md.handlebars",
        "{projectRoot}/GETSTARTED.md",
        "{projectRoot}/GUIDES.md",
        "{projectRoot}/package.json",
        "{projectRoot}/collection.json",
        "{projectRoot}/generators.json",
        "{projectRoot}/executors.json",
        "{projectRoot}/builders.json"
      ],
      "outputs": [
        "{projectRoot}/README.md"
      ]
    }
  },
  "affected": {
    "defaultBase": "development"
  },
  "generators": {
    "@nx/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "none",
      "tags": "angular,ngx",
      "prefix": "rxap"
    },
    "@nx/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest",
      "publishable": true,
      "addTailwind": true,
      "changeDetection": "OnPush",
      "standalone": true,
      "style": "scss",
      "directory": "angular",
      "tags": "angular,ngx",
      "prefix": "rxap"
    },
    "@nx/plugin:plugin": {
      "directory": "plugin",
      "publishable": true,
      "tags": "plugin,nx,nx-plugin"
    },
    "@nx/js:library": {
      "unitTestRunner": "jest",
      "publishable": true
    },
    "@nx/angular:component": {
      "style": "scss"
    },
    "@nx/nest:library": {
      "directory": "nest",
      "tags": "nest",
      "publishable": true
    }
  },
  "workspaceLayout": {
    "appsDir": "demos",
    "libsDir": "packages"
  }
}
