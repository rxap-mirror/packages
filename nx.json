{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx-cloud",
      "options": {
        "parallel": 32,
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "compodoc",
          "typedoc",
          "component-test",
          "swagger-build",
          "swagger-generate"
        ],
        "accessToken": "YjMxMDI0NDUtYmZmMC00MzE4LWEyZjAtNWFlZDM1ODc1MTQ1fHJlYWQtd3JpdGU="
      }
    }
  },
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*"
    ],
    "typescript": [
      "{projectRoot}/**/*.ts",
      "{projectRoot}/tsconfig.json",
      "{projectRoot}/tsconfig.*.json"
    ],
    "test": [
      "typescript",
      "{projectRoot}/src/**/*",
      "!{projectRoot}/tsconfig.lib.json",
      "!{projectRoot}/tsconfig.lib.prod.json"
    ],
    "production": [
      "typescript",
      "{projectRoot}/src/**/*",
      "!{projectRoot}/**/*.{spec,stories,cy}.ts",
      "!{projectRoot}/jest.config.ts",
      "!{projectRoot}/src/test-setup.[jt]s",
      "!{projectRoot}/tsconfig.spec.json",
      "!{projectRoot}/cypress/**/*",
      "!{projectRoot}/**/*.cy.[jt]s?(x)",
      "!{projectRoot}/cypress.config.[jt]s"
    ],
    "build": [
      "production",
      "{projectRoot}/package.json",
      "{projectRoot}/collection.json",
      "{projectRoot}/generators.json",
      "{projectRoot}/executors.json"
    ]
  },
  "targetDefaults": {
    "build-info": {
      "executor": "@rxap/plugin-application:build-info"
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "inputs": [
        "default",
        "{workspaceRoot}/.eslintrc.json",
        "{workspaceRoot}/.eslintignore"
      ]
    },
    "fix-dependencies": {
      "inputs": [
        "typescript"
      ],
      "dependsOn": [
        "^fix-dependencies"
      ]
    },
    "update-package-group": {
      "inputs": [
        "production"
      ]
    },
    "update-dependencies": {
      "inputs": [
        "{workspaceRoot}/package.json",
        "{projectRoot}/package.json"
      ]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "inputs": [
        "test",
        "^test",
        "{workspaceRoot}/jest.preset.js",
        "{workspaceRoot}/jest.preset.ts"
      ],
      "options": {
        "passWithNoTests": true
      }
    },
    "serve": {
      "dependsOn": [
        "build-info"
      ]
    },
    "swagger-build": {
      "executor": "@nx/webpack:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "transformers": [
          "@nestjs/swagger/plugin"
        ],
        "compiler": "tsc",
        "target": "node"
      }
    },
    "build": {
      "dependsOn": [
        "^build",
        "readme",
        "index-export",
        "check-version",
        "build-info",
        "build-tailwind",
        "generate-package-json",
        "check-ng-package",
        "expose-as-schematic"
      ],
      "inputs": [
        "build",
        "^build"
      ]
    },
    "build-tailwind": {
      "dependsOn": [
        {
          "target": "build",
          "projects": [
            "browser-tailwind"
          ]
        }
      ],
      "inputs": [
        "{projectRoot}/**/*.html",
        "{projectRoot}/**/*.scss",
        "{projectRoot}/**/*.css"
      ],
      "outputs": [
        "{projectRoot}/theme.css"
      ]
    },
    "readme": {
      "inputs": [
        "{projectRoot}/README.md.handlebars",
        "{projectRoot}/GETSTARTED.md",
        "{projectRoot}/GUIDES.md",
        "{projectRoot}/package.json",
        "{projectRoot}/collection.json",
        "{projectRoot}/generators.json",
        "{projectRoot}/executors.json",
        "{projectRoot}/builders.json"
      ],
      "outputs": [
        "{projectRoot}/README.md"
      ]
    },
    "docker": {
      "executor": "@rxap/plugin-docker:build",
      "dependsOn": [
        "build"
      ],
      "options": {
        "imageName": "rxap/packages",
        "imageRegistry": "registry.gitlab.com"
      }
    },
    "swagger-generate": {
      "executor": "@nx/js:node",
      "outputs": [
        "dist/swagger/{projectRoot}/open-api.json"
      ],
      "options": {
        "watch": false
      }
    },
    "save": {
      "dependsOn": [
        "docker"
      ]
    },
    "component-test": {
      "inputs": [
        "default",
        "^production"
      ]
    },
    "docker-save": {
      "dependsOn": [
        "docker"
      ],
      "executor": "@rxap/plugin-docker:save"
    },
    "check-ng-package": {
      "inputs": [
        "{projectRoot}/ng-package.json",
        "{projectRoot}/package.json"
      ]
    },
    "config": {
      "executor": "@rxap/plugin-application:config"
    },
    "generate-package-json": {
      "executor": "@rxap/plugin-nestjs:package-json"
    },
    "generate-open-api": {
      "dependsOn": [
        "swagger-generate"
      ],
      "executor": "@rxap/plugin-library:run-generator",
      "options": {
        "generator": "@rxap/plugin-open-api:generate",
        "options": {
          "project": "open-api-{projectName}",
          "path": "dist/swagger/{projectRoot}/openapi.json",
          "serverId": "{projectName}"
        }
      }
    },
    "index-export": {
      "executor": "@rxap/plugin-library:run-generator",
      "outputs": [
        "{workspaceRoot}/{projectRoot}/src/index.ts"
      ],
      "options": {
        "generator": "@rxap/plugin-library:index-export"
      }
    }
  },
  "affected": {
    "defaultBase": "development"
  },
  "generators": {
    "@nx/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "none",
      "tags": "angular,ngx",
      "prefix": "rxap",
      "standalone": true,
      "addTailwind": true,
      "routing": true
    },
    "@nx/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest",
      "publishable": true,
      "addTailwind": true,
      "changeDetection": "OnPush",
      "standalone": true,
      "style": "scss",
      "directory": "angular",
      "tags": "angular,ngx",
      "prefix": "rxap",
      "skipModule": true
    },
    "@nx/plugin:plugin": {
      "directory": "plugin",
      "publishable": true,
      "tags": "plugin,nx,nx-plugin"
    },
    "@nx/js:library": {
      "unitTestRunner": "jest",
      "publishable": true
    },
    "@nx/angular:component": {
      "style": "scss",
      "standalone": true
    },
    "@nx/nest:library": {
      "directory": "nest",
      "tags": "nest",
      "publishable": true
    },
    "@nx/angular:directive": {
      "standalone": true,
      "skipTests": true
    },
    "@nx/nest:application": {
      "e2eTestRunner": "none",
      "tags": "nest",
      "strict": true
    },
    "@rxap/schematic-nestjs:frontend-microservice": {
      "imageName": "rxap/packages",
      "apiConfigurationFile": "applications/service/configuration/src/assets/latest/config.api.json"
    },
    "@rxap/schematic-nestjs:feature-microservice": {
      "imageName": "rxap/packages",
      "apiConfigurationFile": "applications/service/configuration/src/assets/latest/config.api.json"
    },
    "@rxap/schematic-nestjs:microservice": {
      "imageName": "rxap/packages",
      "apiConfigurationFile": "applications/service/configuration/src/assets/latest/config.api.json"
    },
    "@nx/plugin:executor": {
      "unitTestRunner": "none"
    },
    "@nx/plugin:generator": {
      "unitTestRunner": "none"
    }
  },
  "workspaceLayout": {
    "appsDir": "applications",
    "libsDir": "packages"
  }
}
