{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx-cloud",
      "options": {
        "parallel": 32,
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "compodoc",
          "typedoc",
          "component-test",
          "swagger-build",
          "swagger-generate"
        ],
        "accessToken": "YjMxMDI0NDUtYmZmMC00MzE4LWEyZjAtNWFlZDM1ODc1MTQ1fHJlYWQtd3JpdGU="
      }
    }
  },
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*"
    ],
    "typescript": [
      "{projectRoot}/**/*.ts",
      "{projectRoot}/tsconfig.json",
      "{projectRoot}/tsconfig.*.json"
    ],
    "test": [
      "typescript",
      "{projectRoot}/src/**/*",
      "!{projectRoot}/tsconfig.lib.json",
      "!{projectRoot}/tsconfig.lib.prod.json"
    ],
    "production": [
      "typescript",
      "{projectRoot}/src/**/*",
      "!{projectRoot}/**/*.{spec,stories,cy}.ts",
      "!{projectRoot}/jest.config.ts",
      "!{projectRoot}/src/test-setup.[jt]s",
      "!{projectRoot}/tsconfig.spec.json",
      "!{projectRoot}/cypress/**/*",
      "!{projectRoot}/**/*.cy.[jt]s?(x)",
      "!{projectRoot}/cypress.config.[jt]s"
    ]
  },
  "targetDefaults": {
    "lint": {
      "inputs": [
        "default",
        "{workspaceRoot}/.eslintrc.json",
        "{workspaceRoot}/.eslintignore"
      ]
    },
    "fix-dependencies": {
      "inputs": [
        "typescript"
      ],
      "dependsOn": [
        "^fix-dependencies"
      ]
    },
    "update-package-group": {
      "inputs": [
        "production"
      ]
    },
    "update-dependencies": {
      "inputs": [
        "{workspaceRoot}/package.json",
        "{projectRoot}/package.json"
      ]
    },
    "test": {
      "inputs": [
        "test",
        "^test",
        "{workspaceRoot}/jest.preset.js",
        "{workspaceRoot}/jest.preset.ts"
      ]
    },
    "serve": {
      "dependsOn": [
        "build-info"
      ]
    },
    "build": {
      "dependsOn": [
        "^build",
        "readme",
        "index-export",
        "check-version",
        "build-info",
        "build-tailwind",
        "generate-package-json",
        "check-ng-package"
      ],
      "inputs": [
        "production",
        "{projectRoot}/package.json",
        "{projectRoot}/collection.json",
        "{projectRoot}/generators.json",
        "{projectRoot}/executors.json",
        "^production"
      ]
    },
    "build-tailwind": {
      "dependsOn": [
        {
          "target": "build",
          "projects": [
            "browser-tailwind"
          ]
        }
      ],
      "inputs": [
        "{projectRoot}/**/*.html",
        "{projectRoot}/**/*.scss",
        "{projectRoot}/**/*.css"
      ],
      "outputs": [
        "{projectRoot}/theme.css"
      ]
    },
    "readme": {
      "inputs": [
        "{projectRoot}/README.md.handlebars",
        "{projectRoot}/GETSTARTED.md",
        "{projectRoot}/GUIDES.md",
        "{projectRoot}/package.json",
        "{projectRoot}/collection.json",
        "{projectRoot}/generators.json",
        "{projectRoot}/executors.json",
        "{projectRoot}/builders.json"
      ],
      "outputs": [
        "{projectRoot}/README.md"
      ]
    },
    "docker": {
      "dependsOn": [
        "build"
      ]
    },
    "save": {
      "dependsOn": [
        "docker"
      ]
    },
    "component-test": {
      "inputs": [
        "default",
        "^production"
      ]
    },
    "docker-save": {
      "dependsOn": [
        "docker"
      ]
    },
    "check-ng-package": {
      "inputs": [
        "{projectRoot}/ng-package.json",
        "{projectRoot}/package.json"
      ]
    }
  },
  "affected": {
    "defaultBase": "development"
  },
  "generators": {
    "@nx/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "none",
      "tags": "angular,ngx",
      "prefix": "rxap"
    },
    "@nx/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest",
      "publishable": true,
      "addTailwind": true,
      "changeDetection": "OnPush",
      "standalone": true,
      "style": "scss",
      "directory": "angular",
      "tags": "angular,ngx",
      "prefix": "rxap",
      "skipModule": true
    },
    "@nx/plugin:plugin": {
      "directory": "plugin",
      "publishable": true,
      "tags": "plugin,nx,nx-plugin"
    },
    "@nx/js:library": {
      "unitTestRunner": "jest",
      "publishable": true
    },
    "@nx/angular:component": {
      "style": "scss"
    },
    "@nx/nest:library": {
      "directory": "nest",
      "tags": "nest",
      "publishable": true
    },
    "@nx/angular:directive": {
      "standalone": true,
      "skipTests": true
    },
    "@nx/nest:application": {
      "e2eTestRunner": "none"
    },
    "@rxap/schematic-nestjs:frontend-microservice": {
      "imageName": "rxap/packages",
      "apiConfigurationFile": "applications/service/configuration/src/assets/latest/config.api.json"
    },
    "@rxap/schematic-nestjs:feature-microservice": {
      "imageName": "rxap/packages",
      "apiConfigurationFile": "applications/service/configuration/src/assets/latest/config.api.json"
    },
    "@rxap/schematic-nestjs:microservice": {
      "imageName": "rxap/packages",
      "apiConfigurationFile": "applications/service/configuration/src/assets/latest/config.api.json"
    },
    "@nx/plugin:executor": {
      "unitTestRunner": "none"
    },
    "@nx/plugin:generator": {
      "unitTestRunner": "none"
    }
  },
  "workspaceLayout": {
    "appsDir": "applications",
    "libsDir": "packages"
  }
}
