{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx-cloud",
      "options": {
        "parallel": 32,
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "compodoc",
          "typedoc",
          "component-test",
          "swagger-build",
          "swagger-generate",
          "generate-package-json",
          "index-export",
          "update-dependencies",
          "update-package-group",
          "check-version",
          "check-ng-package",
          "generate-open-api",
          "ci-info",
          "readme",
          "build-tailwind",
          "localazy-upload",
          "extract-i18n",
          "expose-as-schematic",
          "localazy-download",
          "fix-dependencies"
        ],
        "accessToken": "YjMxMDI0NDUtYmZmMC00MzE4LWEyZjAtNWFlZDM1ODc1MTQ1fHJlYWQtd3JpdGU="
      }
    }
  },
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*"
    ],
    "typescript": [
      "{projectRoot}/**/*.ts",
      "{projectRoot}/tsconfig.json",
      "{projectRoot}/tsconfig.*.json"
    ],
    "test": [
      "typescript",
      "{projectRoot}/src/**/*",
      "!{projectRoot}/tsconfig.lib.json",
      "!{projectRoot}/tsconfig.lib.prod.json"
    ],
    "production": [
      "typescript",
      "{projectRoot}/src/**/*",
      "!{projectRoot}/**/*.{spec,stories,cy}.ts",
      "!{projectRoot}/jest.config.ts",
      "!{projectRoot}/src/test-setup.[jt]s",
      "!{projectRoot}/tsconfig.spec.json",
      "!{projectRoot}/cypress/**/*",
      "!{projectRoot}/**/*.cy.[jt]s?(x)",
      "!{projectRoot}/cypress.config.[jt]s"
    ],
    "build": [
      "production",
      "{projectRoot}/package.json",
      "{projectRoot}/collection.json",
      "{projectRoot}/generators.json",
      "{projectRoot}/executors.json"
    ]
  },
  "targetDefaults": {
    "build-info": {
      "executor": "@rxap/plugin-application:build-info"
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "inputs": [
        "default",
        "{workspaceRoot}/.eslintrc.json",
        "{workspaceRoot}/.eslintignore"
      ]
    },
    "fix-dependencies": {
      "inputs": [
        "production",
        "^production",
        "{projectRoot}/package.json"
      ],
      "dependsOn": [
        "^fix-dependencies"
      ],
      "executor": "@rxap/plugin-library:run-generator",
      "outputs": [
        "{projectRoot}/package.json"
      ],
      "options": {
        "generator": "@rxap/plugin-library:fix-dependencies",
        "options": {
          "strict": true
        }
      }
    },
    "update-package-group": {
      "executor": "@rxap/plugin-library:update-package-group",
      "dependsOn": [
        "update-dependencies"
      ],
      "inputs": [
        "production",
        "{projectRoot}/package.json"
      ],
      "outputs": [
        "{projectRoot}/package.json"
      ]
    },
    "update-dependencies": {
      "executor": "@rxap/plugin-library:update-dependencies",
      "outputs": [
        "{projectRoot}/package.json"
      ],
      "inputs": [
        "{workspaceRoot}/package.json",
        "{projectRoot}/package.json"
      ]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "inputs": [
        "test",
        "^test",
        "{workspaceRoot}/jest.preset.js",
        "{workspaceRoot}/jest.preset.ts"
      ],
      "options": {
        "passWithNoTests": true
      }
    },
    "serve": {
      "dependsOn": [
        "build-info"
      ]
    },
    "swagger-build": {
      "executor": "@nx/webpack:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "transformers": [
          "@nestjs/swagger/plugin"
        ],
        "compiler": "tsc",
        "target": "node"
      },
      "inputs": [
        "build",
        "^build"
      ]
    },
    "build": {
      "dependsOn": [
        "^build",
        "readme",
        "index-export",
        "check-version",
        "build-tailwind",
        "generate-package-json",
        "check-ng-package",
        "localazy-download",
        "build-info"
      ],
      "inputs": [
        "build",
        "^build"
      ]
    },
    "localazy-download": {
      "executor": "@rxap/plugin-localazy:download",
      "inputs": [
        {
          "runtime": "date"
        },
        {
          "env": "CI_COMMIT_TIMESTAMP"
        },
        {
          "env": "CI_COMMIT_SHA"
        },
        {
          "env": "CI_JOB_ID"
        },
        {
          "env": "CI_PIPELINE_ID"
        }
      ],
      "outputs": [
        "{projectRoot}/src/i18n"
      ]
    },
    "expose-as-schematic": {
      "executor": "@rxap/plugin-library:run-generator",
      "options": {
        "generator": "@rxap/plugin-library:expose-as-schematic"
      },
      "outputs": [
        "{projectRoot}/generators.json",
        "{projectRoot}/src/generators/**/index.ts"
      ],
      "inputs": [
        "{projectRoot}/generators.json",
        "{projectRoot}/src/generators/**/schema.json"
      ]
    },
    "extract-i18n": {
      "outputs": [
        "{options.outputPath}"
      ],
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "format": "xliff2"
      },
      "input": [
        "production",
        "^production"
      ]
    },
    "build-tailwind": {
      "executor": "@rxap/plugin-angular:tailwind",
      "configurations": {
        "production": {
          "minify": true
        }
      },
      "dependsOn": [
        {
          "target": "build",
          "projects": [
            "browser-tailwind"
          ]
        }
      ],
      "inputs": [
        "{projectRoot}/**/*.html",
        "{projectRoot}/**/*.scss",
        "{projectRoot}/**/*.css"
      ],
      "outputs": [
        "{projectRoot}/theme.css"
      ]
    },
    "readme": {
      "executor": "@rxap/plugin-library:readme",
      "inputs": [
        "{projectRoot}/README.md.handlebars",
        "{projectRoot}/GETSTARTED.md",
        "{projectRoot}/GUIDES.md",
        "{projectRoot}/package.json",
        "{projectRoot}/collection.json",
        "{projectRoot}/generators.json",
        "{projectRoot}/executors.json",
        "{projectRoot}/builders.json"
      ],
      "outputs": [
        "{projectRoot}/README.md"
      ]
    },
    "docker": {
      "executor": "@rxap/plugin-docker:build",
      "inputs": [
        "production",
        "^production",
        "{projectRoot}/package.json"
      ],
      "dependsOn": [
        "build"
      ],
      "options": {
        "imageName": "rxap/packages",
        "imageRegistry": "registry.gitlab.com"
      }
    },
    "swagger-generate": {
      "executor": "@nx/js:node",
      "outputs": [
        "dist/swagger/{projectRoot}/openapi.json"
      ],
      "options": {
        "watch": false
      },
      "inputs": [
        "{workspaceRoot}/dist/swagger/{projectRoot}/main.js",
        "{workspaceRoot}/dist/swagger/{projectRoot}/main.js.map"
      ]
    },
    "check-version": {
      "executor": "@rxap/plugin-library:check-version",
      "inputs": [
        "{projectRoot}/package.json",
        "{workspaceRoot}/package.json"
      ]
    },
    "component-test": {
      "executor": "@nx/cypress:cypress",
      "inputs": [
        "default",
        "^production"
      ],
      "configurations": {
        "open": {
          "watch": true
        }
      },
      "options": {
        "testingType": "component",
        "skipServe": true,
        "devServerTarget": "angular:build:development"
      }
    },
    "docker-save": {
      "dependsOn": [
        "docker"
      ],
      "executor": "@rxap/plugin-docker:save"
    },
    "check-ng-package": {
      "executor": "@rxap/plugin-angular:check-ng-package",
      "inputs": [
        "{projectRoot}/ng-package.json",
        "{projectRoot}/package.json"
      ]
    },
    "config": {
      "executor": "@rxap/plugin-application:config"
    },
    "generate-package-json": {
      "executor": "@rxap/plugin-nestjs:package-json",
      "outputs": [
        "{projectRoot}/package.json"
      ],
      "inputs": [
        "production",
        "^production"
      ]
    },
    "generate-open-api": {
      "dependsOn": [
        "swagger-generate"
      ],
      "executor": "@rxap/plugin-library:run-generator",
      "options": {
        "generator": "@rxap/plugin-open-api:generate",
        "options": {
          "project": "open-api-{projectName}",
          "path": "dist/swagger/{projectRoot}/openapi.json",
          "serverId": "{projectName}"
        }
      },
      "inputs": [
        "{workspaceRoot}/dist/swagger/{projectRoot}/openapi.json"
      ],
      "outputs": [
        "{workspaceRoot}/packages/open-api/{projectRoot}/src"
      ]
    },
    "index-export": {
      "executor": "@rxap/plugin-library:run-generator",
      "outputs": [
        "{workspaceRoot}/{projectRoot}/src/index.ts"
      ],
      "options": {
        "generator": "@rxap/plugin-library:index-export"
      },
      "inputs": [
        "production"
      ]
    },
    "localazy-upload": {
      "dependsOn": [
        "extract-i18n"
      ],
      "inputs": [
        "{projectRoot}/src/i18n/messages.xlf"
      ]
    }
  },
  "affected": {
    "defaultBase": "development"
  },
  "generators": {
    "@nx/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "none",
      "tags": "angular,ngx",
      "prefix": "rxap",
      "standalone": true,
      "addTailwind": true,
      "routing": true
    },
    "@nx/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest",
      "publishable": true,
      "addTailwind": true,
      "changeDetection": "OnPush",
      "standalone": true,
      "style": "scss",
      "directory": "angular",
      "tags": "angular,ngx",
      "prefix": "rxap",
      "skipModule": true
    },
    "@nx/plugin:plugin": {
      "directory": "plugin",
      "publishable": true,
      "tags": "plugin,nx,nx-plugin"
    },
    "@nx/js:library": {
      "unitTestRunner": "jest",
      "publishable": true
    },
    "@nx/angular:component": {
      "style": "scss",
      "standalone": true
    },
    "@nx/nest:library": {
      "directory": "nest",
      "tags": "nest",
      "publishable": true
    },
    "@nx/angular:directive": {
      "standalone": true,
      "skipTests": true
    },
    "@nx/nest:application": {
      "e2eTestRunner": "none",
      "tags": "nest",
      "strict": true
    },
    "@nx/plugin:executor": {
      "unitTestRunner": "none"
    },
    "@nx/plugin:generator": {
      "unitTestRunner": "none"
    },
    "@nx/angular:library-secondary-entry-point": {
      "skipModule": true
    }
  },
  "workspaceLayout": {
    "appsDir": "applications",
    "libsDir": "packages"
  }
}
