{
  "name": "service-configuration",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "applications/service/configuration/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/applications/service/configuration",
        "main": "applications/service/configuration/src/main.ts",
        "tsConfig": "applications/service/configuration/tsconfig.app.json",
        "assets": [
          "applications/service/configuration/src/Dockerfile",
          "applications/service/configuration/src/healthcheck.js",
          "applications/service/configuration/src/build.json",
          "applications/service/configuration/src/assets/.gitkeep"
        ],
        "generatePackageJson": true,
        "target": "node",
        "compiler": "tsc"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "applications/service/configuration/src/environments/environment.ts",
              "with": "applications/service/configuration/src/environments/environment.prod.ts"
            }
          ]
        },
        "development": {
          "assets": [
            "applications/service/configuration/src/build.json",
            "applications/service/configuration/src/assets"
          ]
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "service-configuration:build:development"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "applications/service/configuration/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/applications/service/configuration"
      ],
      "options": {
        "jestConfig": "applications/service/configuration/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "ci": {
      "executor": "@rxap/plugin-application:build-info",
      "configurations": {
        "production": {}
      }
    },
    "docker": {
      "executor": "@rxap/plugin-docker:build",
      "options": {
        "imageName": "rxap/packages",
        "imageSuffix": "/service/configuration",
        "imageRegistry": "registry.gitlab.com"
      },
      "configurations": {
        "production": {},
        "development": {}
      }
    },
    "swagger-build": {
      "executor": "@nx/webpack:webpack",
      "options": {
        "outputPath": "dist/swagger/service/configuration",
        "main": "applications/service/configuration/src/swagger.ts",
        "transformers": [
          "@nestjs/swagger/plugin"
        ],
        "tsConfig": "applications/service/configuration/tsconfig.app.json",
        "fileReplacements": [
          {
            "replace": "applications/service/configuration/src/environments/environment.ts",
            "with": "applications/service/configuration/src/environments/environment.swagger.ts"
          }
        ],
        "compiler": "tsc",
        "target": "node"
      }
    },
    "build-info": {
      "executor": "@rxap/plugin-application:build-info",
      "options": {},
      "configurations": {
        "production": {},
        "development": {}
      }
    },
    "docker-save": {
      "executor": "@rxap/plugin-docker:save",
      "options": {},
      "configurations": {
        "production": {},
        "development": {}
      }
    },
    "generate-package-json": {
      "executor": "@rxap/plugin-nestjs:package-json",
      "configurations": {
        "production": {}
      }
    },
    "generate-open-api": {
      "executor": "@rxap/plugin-library:run-generator",
      "options": {
        "generator": "@rxap/schematics-open-api:generate",
        "options": {
          "project": "open-api-service-configuration",
          "path": "dist/swagger/applications/service/configuration/openapi.json",
          "serverId": "service-configuration"
        }
      }
    }
  },
  "tags": [
    "service",
    "nest",
    "sentry",
    "open-api"
  ]
}
