{
  "name": "service-app-angular-accordion",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "applications/service/app/angular/accordion/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/applications/service/app/angular/accordion",
        "main": "applications/service/app/angular/accordion/src/main.ts",
        "tsConfig": "applications/service/app/angular/accordion/tsconfig.app.json",
        "assets": [
          "applications/service/app/angular/accordion/src/assets"
        ],
        "isolatedConfig": true,
        "webpackConfig": "applications/service/app/angular/accordion/webpack.config.js",
        "generatePackageJson": true
      },
      "configurations": {
        "development": {
          "progress": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "applications/service/app/angular/accordion/src/environments/environment.ts",
              "with": "applications/service/app/angular/accordion/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "service-app-angular-accordion:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "service-app-angular-accordion:build:development"
        },
        "production": {
          "buildTarget": "service-app-angular-accordion:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "applications/service/app/angular/accordion/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "applications/service/app/angular/accordion/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "generate-package-json": {
      "executor": "@rxap/plugin-nestjs:package-json",
      "configurations": {
        "production": {}
      }
    },
    "generate-open-api": {
      "executor": "@rxap/plugin-library:run-generator",
      "options": {
        "generator": "@rxap/plugin-open-api:generate",
        "options": {
          "project": "open-api-service-app-angular-accordion",
          "path": "dist/swagger/applications/service/app/angular/accordion/openapi.json",
          "serverId": "service-app-angular-accordion"
        }
      }
    },
    "swagger-build": {
      "executor": "@nx/webpack:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/swagger/applications/service/app/angular/accordion",
        "main": "applications/service/app/angular/accordion/src/swagger.ts",
        "target": "node",
        "compiler": "tsc",
        "webpackConfig": "applications/service/app/angular/accordion/webpack.config.js",
        "transformers": [
          "@nestjs/swagger/plugin"
        ],
        "tsConfig": "applications/service/app/angular/accordion/tsconfig.app.json",
        "fileReplacements": [
          {
            "replace": "applications/service/app/angular/accordion/src/environments/environment.ts",
            "with": "applications/service/app/angular/accordion/src/environments/environment.swagger.ts"
          }
        ]
      }
    },
    "swagger-generate": {
      "executor": "@nx/js:node",
      "outputs": [
        "dist/swagger/applications/service/app/angular/accordion/openapi.json"
      ],
      "options": {
        "buildTarget": "service-app-angular-accordion:swagger-build",
        "watch": false
      }
    }
  },
  "tags": [
    "nest",
    "backend",
    "service",
    "sentry",
    "swagger",
    "health-indicator",
    "express"
  ]
}
