<table>
  <actions>
    <http-action type="delete" refresh="true" method="DELETE" url="http://google.com/{{row.id}}" icon="delete">
      <show key="__archived">true</show>
    </http-action>
    <navigate-action icon="edit" routerLink="{{row.id}}"/>
  </actions>
  <columns>
    <column id="userId">
      <header>UserId</header>
    </column>
    <column id="title" width="500">
      <header>Title</header>
    </column>
    <column id="body">
      <header>Body</header>
    </column>
  </columns>
</table>
