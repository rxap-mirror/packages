{
  "name": "xml-parser",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/xml-parser/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": [
        "{workspaceRoot}/dist/{projectRoot}"
      ],
      "options": {
        "project": "packages/xml-parser/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "packages/xml-parser/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "packages/xml-parser/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "fix-dependencies": {
      "options": {
        "options": {
          "strict": true
        }
      }
    },
    "lint": {
      "options": {
        "lintFilePatterns": [
          "packages/xml-parser/**/*.ts"
        ]
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs xml-parser {args.ver} {args.tag}",
      "dependsOn": [
        "build"
      ]
    },
    "readme": {},
    "test": {
      "options": {
        "jestConfig": "packages/xml-parser/jest.config.ts"
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "update-dependencies": {},
    "update-package-group": {}
  },
  "tags": [
    "packages",
    "scope:utilities",
    "standalone"
  ]
}
