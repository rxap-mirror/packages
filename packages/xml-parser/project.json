{
  "name": "xml-parser",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/xml-parser/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": [
        "{workspaceRoot}/dist/{projectRoot}"
      ],
      "options": {
        "project": "packages/xml-parser/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "packages/xml-parser/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "packages/xml-parser/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs xml-parser {args.ver} {args.tag}",
      "dependsOn": [
        "build"
      ]
    },
    "lint": {
      "options": {
        "lintFilePatterns": [
          "packages/xml-parser/**/*.ts"
        ]
      }
    },
    "test": {
      "options": {
        "jestConfig": "packages/xml-parser/jest.config.ts"
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "update-dependencies": {
      "executor": "@rxap/plugin-library:update-dependencies"
    },
    "update-package-group": {
      "executor": "@rxap/plugin-library:update-package-group"
    },
    "readme": {
      "executor": "@rxap/plugin-library:readme"
    },
    "fix-dependencies": {
      "executor": "@rxap/plugin-library:run-generator",
      "outputs": [
        "{workspaceRoot}/{projectRoot}/package.json"
      ],
      "options": {
        "generator": "@rxap/plugin-library:fix-dependencies",
        "options": {
          "strict": true
        }
      }
    }
  },
  "tags": [
    "packages"
  ]
}
