{
  "description": "Adds the validator features to the project",
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "validator",
  "type": "object",
  "properties": {
    "project": {
      "type": "string",
      "description": "The name of the project.",
      "$default": {
        "$source": "projectName"
      },
      "x-prompt": "Which project should be extend with sentry support?"
    }
  },
  "required": [
    "project"
  ]
}
