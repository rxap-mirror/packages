{
  "$schema": "http://json-schema.org/schema",
  "$id": "type",
  "oneOf": [
    {
      "type": "string"
    },
    {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "isTypeOnly": {
          "type": "boolean"
        },
        "moduleSpecifier": {
          "type": "string"
        },
        "namedImport": {
          "type": "string"
        },
        "namespaceImport": {
          "type": "string"
        },
        "defaultImport": {
          "type": "string"
        }
      },
      "required": [
        "name"
      ]
    }
  ]
}
