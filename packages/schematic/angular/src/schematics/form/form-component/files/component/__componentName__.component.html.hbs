<form rxapForm class="flex flex-col gap-2">
  {{#each controlList}}
  {{ indent (compile this.handlebars control=this prefix=../prefix) 2 }}
  {{/each}}
  {{#if window}}
  <ng-template rxapFormWindowFooter let-windowRef>
    <rxap-form-controls
      (close)="windowRef.complete()"
      (submitted)="windowRef.next($event)"
    ></rxap-form-controls>
  </ng-template>
  {{else}}
  <rxap-form-controls></rxap-form-controls>
  {{/if}}
</form>
