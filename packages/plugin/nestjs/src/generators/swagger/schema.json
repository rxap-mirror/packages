{
  "$schema": "http://json-schema.org/schema",
  "$id": "Swagger",
  "title": "",
  "type": "object",
  "properties": {
    "project": {
      "type": "string",
      "description": "The name of the project.",
      "$default": {
        "$source": "projectName"
      },
      "x-prompt": "Which project should be initialized?"
    },
    "overwrite": {
      "type": "boolean"
    },
    "standalone": {
      "type": "boolean",
      "default": false,
      "description": "Whether the nest service should be standalone"
    }
  },
  "required": [
    "project"
  ]
}
