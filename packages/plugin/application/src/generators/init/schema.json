{
  "$schema": "http://json-schema.org/schema",
  "$id": "Init",
  "title": "",
  "type": "object",
  "properties": {
    "projects": {
      "alias": "project",
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "overwrite": {
      "type": "boolean",
      "default": false,
      "description": "Whether to overwrite existing files"
    },
    "skipProjects": {
      "type": "boolean",
      "default": false,
      "description": "Whether to skip executing project specific initialization"
    },
    "authentik": {
      "type": "boolean",
      "default": false,
      "description": "Whether to initialize authentik docker compose setup"
    },
    "minio": {
      "type": "boolean",
      "default": false,
      "description": "Whether to initialize minio docker compose setup"
    }
  },
  "required": []
}
