{
  "$schema": "http://json-schema.org/schema",
  "$id": "InitApplication",
  "title": "",
  "type": "object",
  "properties": {
    "projects": {
      "alias": "project",
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "sentry": {
      "type": "boolean",
      "default": true
    },
    "openApi": {
      "type": "boolean",
      "default": false
    },
    "openApiLegacy": {
      "type": "boolean",
      "default": false
    },
    "config": {
      "type": "boolean",
      "default": true
    },
    "localazy": {
      "type": "boolean",
      "default": false
    },
    "i18n": {
      "type": "boolean",
      "default": false
    },
    "serviceWorker": {
      "type": "boolean",
      "default": false
    },
    "languages": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "material": {
      "type": "boolean",
      "default": true
    },
    "generateMain": {
      "type": "boolean",
      "default": false
    },
    "overwrite": {
      "type": "boolean",
      "default": false,
      "description": "Whether to overwrite existing files"
    },
    "cleanup": {
      "type": "boolean",
      "default": false,
      "description": "Whether to cleanup files"
    },
    "monolithic": {
      "type": "boolean",
      "default": false,
      "description": "Whether to generate a monolithic application"
    },
    "localazyReadKey": {
      "type": "string",
      "description": "Localazy read key"
    },
    "authentik": {
      "type": "boolean",
      "default": false,
      "description": "Use authentik for authentication"
    },
    "oauth": {
      "type": "boolean",
      "default": false,
      "description": "Use OAuth for authentication"
    }
  },
  "required": []
}
