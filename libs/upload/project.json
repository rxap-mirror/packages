{
  "name": "upload",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/upload/src",
  "prefix": "rxap",
  "targets": {
    "version": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["echo ok"]
      },
      "configurations": {
        "production": {}
      }
    },
    "build": {
      "executor": "@nx/angular:package",
      "options": {
        "tsConfig": "libs/upload/tsconfig.lib.json",
        "project": "libs/upload/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/upload/tsconfig.lib.prod.json"
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["coverage/libs/upload"],
      "options": {
        "jestConfig": "libs/upload/jest.config.js",
        "passWithNoTests": true
      }
    },
    "schematics": {
      "executor": "@rxap/plugin-library-schematics:build",
      "options": {
        "buildTarget": "upload:build:production",
        "skipBuild": false,
        "tsConfig": "libs/upload/tsconfig.schematics.json"
      }
    },
    "update-package-group": {
      "executor": "@rxap/plugin-library:update-package-group"
    },
    "publish": {
      "executor": "@rxap/plugin-library-publish:publish",
      "options": {
        "buildTarget": "upload:build:production",
        "preTarget": "upload:pack"
      }
    },
    "readme": {
      "executor": "@rxap/plugin-readme-generator:library"
    },
    "build-schematics": {
      "executor": "@rxap/plugin-library:build-schematics",
      "options": {
        "outputPath": "dist/libs/upload",
        "tsConfig": "libs/upload/tsconfig.schematics.json",
        "assets": [],
        "type": "schematics"
      }
    },
    "build-migrations": {
      "executor": "@rxap/plugin-library:build-schematics",
      "options": {
        "outputPath": "dist/libs/upload",
        "tsConfig": "libs/upload/tsconfig.migrations.json",
        "assets": [],
        "type": "migrations"
      }
    },
    "update-peer-dependencies": {
      "executor": "@rxap/plugin-library:update-peer-dependencies",
      "options": {
        "ignore": [
          "tsconfig-paths-webpack-plugin",
          "jest-preset-angular",
          "ng-mocks",
          "@storybook\\/"
        ]
      }
    },
    "storybook": {
      "executor": "@nx/storybook:storybook",
      "options": {
        "port": 4400,
        "projectBuildConfig": "upload:build-storybook",
        "configDir": "libs/upload/.storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@nx/storybook:build",
      "outputs": ["{options.outputDir}"],
      "options": {
        "projectBuildConfig": "upload:build-storybook",
        "configDir": "libs/upload/.storybook",
        "outputDir": "dist/storybook/upload"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "scss-bundle": {
      "executor": "@rxap/plugin-scss-bundle:build",
      "options": {
        "buildTarget": "upload:build:production",
        "skipBuild": true,
        "ignoreImports": ["~@angular/.*"]
      }
    }
  },
  "tags": []
}
