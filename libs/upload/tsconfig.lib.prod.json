/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.lib.json",
  "compilerOptions": {
    "declarationMap": false,
    "paths": {
      "@rxap/utilities": [
        "dist/libs/utilities"
      ],
      "@rxap/remote-method": [
        "dist/libs/remote-method"
      ],
      "@rxap/definition": [
        "dist/libs/definition"
      ],
      "@rxap/remote-method/directive": [
        "dist/libs/remote-method/directive"
      ]
    }
  },
  "angularCompilerOptions": {
    "enableIvy": true
  }
}
