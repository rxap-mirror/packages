{
  "name": "authorization",
  "projectType": "library",
  "sourceRoot": "libs/authorization/src",
  "prefix": "rxap",
  "targets": {
    "version": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["echo ok"]
      },
      "configurations": {
        "production": {}
      }
    },
    "build": {
      "executor": "@nrwl/angular:package",
      "options": {
        "tsConfig": "libs/authorization/tsconfig.lib.json",
        "project": "libs/authorization/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/authorization/tsconfig.lib.prod.json"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/authorization/src/**/*.ts",
          "libs/authorization/src/**/*.html"
        ]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/authorization"],
      "options": {
        "jestConfig": "libs/authorization/jest.config.js",
        "passWithNoTests": true
      }
    },
    "pack": {
      "executor": "@rxap/plugin-pack:build",
      "options": {
        "targets": [
          "authorization:build:production",
          "authorization:build-schematics",
          "authorization:build-migrations"
        ]
      }
    },
    "schematics": {
      "executor": "@rxap/plugin-library-schematics:build",
      "options": {
        "buildTarget": "authorization:build:production",
        "skipBuild": false,
        "tsConfig": "libs/authorization/tsconfig.schematics.json"
      }
    },
    "update-package-group": {
      "executor": "@rxap/plugin-library:update-package-group"
    },
    "publish": {
      "executor": "@rxap/plugin-library-publish:publish",
      "options": {
        "buildTarget": "authorization:build:production",
        "preTarget": "authorization:pack"
      }
    },
    "readme": {
      "executor": "@rxap/plugin-readme-generator:library"
    },
    "build-schematics": {
      "executor": "@rxap/plugin-library:build-schematics",
      "options": {
        "outputPath": "dist/libs/authorization",
        "tsConfig": "libs/authorization/tsconfig.schematics.json",
        "assets": [],
        "type": "schematics"
      }
    },
    "build-migrations": {
      "executor": "@rxap/plugin-library:build-schematics",
      "options": {
        "outputPath": "dist/libs/authorization",
        "tsConfig": "libs/authorization/tsconfig.migrations.json",
        "assets": [],
        "type": "migrations"
      }
    },
    "update-peer-dependencies": {
      "executor": "@rxap/plugin-library:update-peer-dependencies",
      "options": {
        "ignore": ["jest-preset-angular", "ng-mocks", "@storybook\\/"]
      }
    }
  },
  "tags": []
}
