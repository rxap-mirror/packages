{
  "name": "oauth",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/oauth/src",
  "prefix": "rxap",
  "targets": {
    "version": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["echo ok"]
      },
      "configurations": {
        "production": {}
      }
    },
    "build": {
      "executor": "@nx/angular:package",
      "options": {
        "tsConfig": "libs/oauth/tsconfig.lib.json",
        "project": "libs/oauth/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/oauth/tsconfig.lib.prod.json"
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/oauth/src/**/*.ts",
          "libs/oauth/src/**/*.html"
        ]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["coverage/libs/oauth"],
      "options": {
        "jestConfig": "libs/oauth/jest.config.js",
        "passWithNoTests": true
      }
    },
    "pack": {
      "executor": "@rxap/plugin-pack:build",
      "options": {
        "targets": [
          "oauth:build:production",
          "oauth:scss-bundle",
          "oauth:build-schematics",
          "oauth:build-migrations"
        ]
      }
    },
    "schematics": {
      "executor": "@rxap/plugin-library-schematics:build",
      "options": {
        "buildTarget": "oauth:build:production",
        "skipBuild": false,
        "tsConfig": "libs/oauth/tsconfig.schematics.json"
      }
    },
    "update-package-group": {
      "executor": "@rxap/plugin-library:update-package-group"
    },
    "publish": {
      "executor": "@rxap/plugin-library-publish:publish",
      "options": {
        "buildTarget": "oauth:build:production",
        "preTarget": "oauth:pack"
      }
    },
    "scss-bundle": {
      "executor": "@rxap/plugin-scss-bundle:build",
      "options": {
        "buildTarget": "oauth:build:production",
        "skipBuild": true,
        "ignoreImports": ["~@angular/.*"]
      }
    },
    "readme": {
      "executor": "@rxap/plugin-readme-generator:library"
    },
    "build-schematics": {
      "executor": "@rxap/plugin-library:build-schematics",
      "options": {
        "outputPath": "dist/libs/oauth",
        "tsConfig": "libs/oauth/tsconfig.schematics.json",
        "assets": [],
        "type": "schematics"
      }
    },
    "build-migrations": {
      "executor": "@rxap/plugin-library:build-schematics",
      "options": {
        "outputPath": "dist/libs/oauth",
        "tsConfig": "libs/oauth/tsconfig.migrations.json",
        "assets": [],
        "type": "migrations"
      }
    },
    "update-peer-dependencies": {
      "executor": "@rxap/plugin-library:update-peer-dependencies",
      "options": {
        "ignore": ["jest-preset-angular", "ng-mocks", "@storybook\\/"]
      }
    }
  },
  "tags": []
}
