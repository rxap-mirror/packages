{
  "projectType": "library",
  "sourceRoot": "libs/material/directives/src",
  "prefix": "rxap",
  "targets": {
    "version": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["echo ok"]
      },
      "configurations": {
        "production": {}
      }
    },
    "build": {
      "executor": "@nrwl/angular:package",
      "options": {
        "tsConfig": "libs/material/directives/tsconfig.lib.json",
        "project": "libs/material/directives/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/material/directives/tsconfig.lib.prod.json"
        }
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/material/directives"],
      "options": {
        "jestConfig": "libs/material/directives/jest.config.js",
        "passWithNoTests": true
      }
    },
    "pack": {
      "executor": "@rxap/plugin-pack:build",
      "options": {
        "targets": [
          "material-directives:build:production",
          "material-directives:scss-bundle",
          "material-directives:build-schematics",
          "material-directives:build-migrations"
        ]
      }
    },
    "schematics": {
      "executor": "@rxap/plugin-library-schematics:build",
      "options": {
        "buildTarget": "material-directives:build:production",
        "skipBuild": false,
        "tsConfig": "libs/material/directives/tsconfig.schematics.json"
      }
    },
    "update-package-group": {
      "executor": "@rxap/plugin-library:update-package-group"
    },
    "publish": {
      "executor": "@rxap/plugin-library-publish:publish",
      "options": {
        "buildTarget": "material-directives:build:production",
        "preTarget": "material-directives:pack"
      }
    },
    "scss-bundle": {
      "executor": "@rxap/plugin-scss-bundle:build",
      "options": {
        "buildTarget": "material-directives:build:production",
        "skipBuild": true,
        "ignoreImports": ["~@angular/.*"]
      }
    },
    "readme": {
      "executor": "@rxap/plugin-readme-generator:library"
    },
    "build-schematics": {
      "executor": "@rxap/plugin-library:build-schematics",
      "options": {
        "outputPath": "dist/libs/material/directives",
        "tsConfig": "libs/material/directives/tsconfig.schematics.json",
        "assets": [],
        "type": "schematics"
      }
    },
    "build-migrations": {
      "executor": "@rxap/plugin-library:build-schematics",
      "options": {
        "outputPath": "dist/libs/material/directives",
        "tsConfig": "libs/material/directives/tsconfig.migrations.json",
        "assets": [],
        "type": "migrations"
      }
    },
    "update-peer-dependencies": {
      "executor": "@rxap/plugin-library:update-peer-dependencies",
      "options": {
        "ignore": ["jest-preset-angular", "ng-mocks", "@storybook\\/"]
      }
    }
  },
  "tags": []
}
