{
  "name": "material-popover-edit",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/material/popover-edit/src",
  "prefix": "rxap",
  "targets": {
    "version": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["echo ok"]
      },
      "configurations": {
        "production": {}
      }
    },
    "build": {
      "executor": "@nx/angular:package",
      "options": {
        "project": "libs/material/popover-edit/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/material/popover-edit/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/material/popover-edit/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["coverage/libs/material/popover-edit"],
      "options": {
        "jestConfig": "libs/material/popover-edit/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/material/popover-edit/src/**/*.ts",
          "libs/material/popover-edit/src/**/*.html"
        ]
      },
      "outputs": ["{options.outputFile}"]
    },
    "pack": {
      "executor": "@rxap/plugin-pack:build",
      "options": {
        "targets": [
          "material-popover-edit:build:production",
          "material-popover-edit:scss-bundle",
          "material-popover-edit:build-schematics",
          "material-popover-edit:build-migrations"
        ]
      }
    },
    "scss-bundle": {
      "executor": "@rxap/plugin-scss-bundle:build",
      "options": {
        "buildTarget": "material-popover-edit:build:production",
        "skipBuild": true,
        "ignoreImports": ["~@angular/.*"]
      }
    },
    "readme": {
      "executor": "@rxap/plugin-readme-generator:library"
    },
    "build-schematics": {
      "executor": "@rxap/plugin-library:build-schematics",
      "options": {
        "outputPath": "dist/libs/material/popover-edit",
        "tsConfig": "libs/material/popover-edit/tsconfig.schematics.json",
        "assets": [],
        "type": "schematics"
      }
    },
    "build-migrations": {
      "executor": "@rxap/plugin-library:build-schematics",
      "options": {
        "outputPath": "dist/libs/material/popover-edit",
        "tsConfig": "libs/material/popover-edit/tsconfig.migrations.json",
        "assets": [],
        "type": "migrations"
      }
    },
    "update-package-group": {
      "executor": "@rxap/plugin-library:update-package-group"
    },
    "update-peer-dependencies": {
      "executor": "@rxap/plugin-library:update-peer-dependencies",
      "options": {
        "ignore": []
      }
    }
  },
  "tags": []
}
