{
  "name": "@rxap/data-grid",
  "version": "10.0.0",
  "peerDependencies": {
    "@angular/cdk": "^10.1.1",
    "@angular/common": "^10.0.6",
    "@angular/core": "^10.0.6",
    "@angular/forms": "^10.0.6",
    "@angular/platform-browser": "^10.0.6",
    "@angular/router": "^10.0.6",
    "@rxap-schematics/utilities": ">=0.12.2",
    "@rxap/definition": ">=10.0.0",
    "@rxap/forms": ">=10.0.0",
    "@rxap/mixin": ">=0.3.1",
    "@rxap/pipes": ">=0.3.6",
    "@rxap/utilities": ">=0.11.0",
    "@rxap/xml-parser": ">=0.9.0",
    "rxjs": "^6.5.5"
  },
  "dependencies": {
    "tslib": "2.0.1"
  },
  "schematics": "./schematics/collection.json",
  "ng-update": {
    "migrations": "./schematics/migration.json",
    "packageGroup": [
      "@rxap/data-grid",
      "@rxap/utilities",
      "@rxap/pipes",
      "@rxap/forms",
      "@rxap/xml-parser",
      "@rxap/mixin",
      "@rxap-schematics/utilities",
      "@rxap/definition"
    ]
  },
  "scripts": {
    "prepublish": "yarn --cwd ../../ nx run data-grid:pack --with-deps",
    "version": "node ../../tools/update-dependencies.js $PWD",
    "preversion": "yarn --cwd ../../ nx run data-grid:build --with-deps"
  },
  "publishConfig": {
    "directory": "../../dist/libs/data-grid"
  }
}