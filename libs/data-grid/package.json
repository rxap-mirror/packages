{
  "name": "@rxap/data-grid",
  "version": "11.0.3",
  "peerDependencies": {
    "@angular/cdk": "^11.0.3",
    "@angular/common": "^11.0.5",
    "@angular/core": "^11.0.5",
    "@angular/forms": "^11.0.5",
    "@angular/platform-browser": "^11.0.5",
    "@angular/router": "^11.0.5",
    "@rxap/definition": ">=11.0.2",
    "@rxap/forms": ">=11.0.5",
    "@rxap/mixin": ">=11.0.3",
    "@rxap/pipes": ">=11.0.2",
    "@rxap/schematics-utilities": ">=11.0.2",
    "@rxap/utilities": ">=11.0.3",
    "@rxap/xml-parser": ">=11.0.2",
    "rxjs": "^6.5.5"
  },
  "dependencies": {
    "tslib": "2.0.1"
  },
  "schematics": "./schematics/collection.json",
  "ng-update": {
    "migrations": "./schematics/migration.json",
    "packageGroup": [
      "@rxap/data-grid",
      "@rxap/definition",
      "@rxap/forms",
      "@rxap/mixin",
      "@rxap/pipes",
      "@rxap/schematics-utilities",
      "@rxap/utilities",
      "@rxap/xml-parser"
    ]
  },
  "scripts": {
    "prepublish": "yarn --cwd ../../ nx run data-grid:pack --with-deps",
    "preversion": "yarn --cwd ../../ nx run data-grid:pack --with-deps"
  },
  "publishConfig": {
    "directory": "../../dist/libs/data-grid"
  }
}