{
  "$schema": "http://json-schema.org/draft-07/schema",
  "id": "Add",
  "title": "",
  "type": "object",
  "properties": {
    "service": {
      "type": "string",
      "description": "",
      "default": "default",
      "$default": {
        "$source": "argv",
        "index": 1
      },
      "x-prompt": "What service name would you like to use?"
    },
    "automaticScaling": {
      "type": "boolean",
      "description": "Weather the app engine service should have automatic scaling.",
      "alias": "automatic-scaling"
    },
    "maxInstances": {
      "type": "number",
      "description": "The max active instance count.",
      "alias": "max-instances",
      "default": 1
    },
    "minInstances": {
      "type": "number",
      "description": "The min active instance count.",
      "alias": "min-instances",
      "default": 0
    },
    "project": {
      "type": "string",
      "description": "The name of the project.",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "For which project should the app engine deploy builder be added?"
    }
  },
  "required": []
}
