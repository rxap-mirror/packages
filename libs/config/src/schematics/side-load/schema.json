{
  "$schema": "http://json-schema.org/schema",
  "$id": "config-side-load",
  "title": "SideLoad",
  "type": "object",
  "properties": {
    "project": {
      "type": "string",
      "description": "The project name"
    },
    "url": {
      "type": "string",
      "description": "Url for the config that should be side loaded"
    },
    "propertyPath": {
      "type": "string",
      "description": "Property path where the config should be added"
    },
    "required": {
      "type": "boolean",
      "default": false
    }
  },
  "required": [
    "project",
    "url",
    "propertyPath"
  ]
}
