{
  "extends": "../../tsconfig.schematics.json",
  "compilerOptions": {
    "rootDir": "schematics",
    "outDir": "../../dist/libs/open-api/schematics",
    "paths": {
      "@rxap/schematics-utilities": [
        "../../dist/libs/schematics/utilities"
      ],
      "@rxap/schematics-open-api": [
        "../../dist/libs/schematics/open-api"
      ],
      "@rxap/schematics-ts-morph": [
        "../../dist/libs/schematics/ts-morph"
      ]
    }
  },
  "include": [
    "schematics/**/*"
  ],
  "exclude": [
    "schematics/*/files/**/*",
"schematics/**/*.spec.ts"
  ]
}
