{
  "extends": "../../../tsconfig.schematics.json",
  "compilerOptions": {
    "rootDir": "schematics",
    "outDir": "../../../dist/libs/amplify/open-api/schematics",
    "paths": {
      "@rxap/xml-parser": [
        "../../../dist/libs/xml-parser"
      ],
      "@rxap/xml-parser/elements": [
        "../../../dist/libs/xml-parser/elements"
      ],
      "@rxap/xml-parser/decorators": [
        "../../../dist/libs/xml-parser/decorators"
      ],
      "@rxap/xml-parser/testing": [
        "../../../dist/libs/xml-parser/testing"
      ],
      "@rxap/schematics-ts-morph": [
        "../../../dist/libs/schematics/ts-morph"
      ],
      "@rxap/schematics-html": [
        "../../../dist/libs/schematics/html"
      ],
      "@rxap/schematics-xml-parser": [
        "../../../dist/libs/schematics/xml-parser"
      ],
      "@rxap/schematics-form/schematics/*": [
        "../../../dist/libs/schematics/form/schematics/*"
      ]
