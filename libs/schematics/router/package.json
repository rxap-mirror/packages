{
  "name": "@rxap/schematics-router",
  "version": "11.0.0",
  "peerDependencies": {
    "@rxap/schematics-utilities": ">=11.0.3",
    "@angular-devkit/schematics": "^11.2.3",
    "@rxap/xml-parser": ">=11.0.3",
    "rxjs": "^6.5.5"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "schematics": "./schematics/collection.json",
  "ng-update": {
    "migrations": "./schematics/migration.json",
    "packageGroup": [
      "@rxap/schematics-router",
      "@rxap/schematics-utilities",
      "@rxap/xml-parser"
    ]
  },
  "scripts": {
    "prepublish": "yarn --cwd ../../../ nx run schematics-router:pack --with-deps",
    "preversion": "yarn --cwd ../../../ nx run schematics-router:pack --with-deps"
  },
  "publishConfig": {
    "directory": "../../../dist/libs/schematics/router"
  }
}