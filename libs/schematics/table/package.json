{
  "name": "@rxap/schematics-table",
  "version": "11.1.0",
  "peerDependencies": {
    "@rxap/mixin": ">=11.0.4",
    "@rxap/schematics-utilities": ">=11.0.3",
    "@rxap/xml-parser": ">=11.0.3"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "schematics": "./schematics/collection.json",
  "ng-update": {
    "migrations": "./schematics/migration.json",
    "packageGroup": [
      "@rxap/schematics-table",
      "@rxap/mixin",
      "@rxap/xml-parser",
      "@rxap/schematics-utilities"
    ]
  },
  "scripts": {
    "prepublish": "yarn --cwd ../../../ nx run schematics-table:pack --with-deps",
    "preversion": "yarn --cwd ../../../ nx run schematics-table:pack --with-deps"
  },
  "publishConfig": {
    "directory": "../../../dist/libs/schematics/table"
  }
}