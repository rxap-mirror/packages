{
  "name": "@rxap/schematics-shared",
  "version": "11.0.2",
  "peerDependencies": {
    "@rxap/schematics-table": ">=11.3.0",
    "@rxap/schematics-utilities": ">=11.2.0",
    "@rxap/xml-parser": ">=11.0.3",
    "rxjs": "^6.5.5"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "schematics": "./schematics/collection.json",
  "ng-update": {
    "migrations": "./schematics/migration.json",
    "packageGroup": [
      "@rxap/schematics-shared",
      "@rxap/schematics-table",
      "@rxap/schematics-utilities",
      "@rxap/xml-parser"
    ]
  },
  "scripts": {
    "prepublish": "yarn --cwd ../../../ nx run schematics-shared:pack --with-deps",
    "preversion": "yarn --cwd ../../../ nx run schematics-shared:pack --with-deps"
  },
  "publishConfig": {
    "directory": "../../../dist/libs/schematics/shared"
  }
}