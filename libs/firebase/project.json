{
  "name": "firebase",
  "projectType": "library",
  "sourceRoot": "libs/firebase/src",
  "prefix": "rxap",
  "targets": {
    "version": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["echo ok"]
      },
      "configurations": {
        "production": {}
      }
    },
    "build": {
      "executor": "@nrwl/angular:package",
      "options": {
        "tsConfig": "libs/firebase/tsconfig.lib.json",
        "project": "libs/firebase/ng-package.json",
        "buildableProjectDepsInPackageJsonType": "dependencies"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/firebase/tsconfig.lib.prod.json"
        }
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "libs/firebase/jest.config.js",
        "passWithNoTests": true
      },
      "outputs": ["coverage/libs/firebase"]
    },
    "compodoc": {
      "executor": "@twittwer/compodoc:compodoc",
      "options": {
        "tsConfig": "libs/firebase/tsconfig.lib.json",
        "outputPath": "dist/compodoc/firebase"
      },
      "configurations": {
        "json": {
          "exportFormat": "json"
        }
      }
    },
    "pack": {
      "executor": "@rxap/plugin-pack:build",
      "options": {
        "targets": [
          "firebase:build:production",
          "firebase:build-schematics",
          "firebase:build-migrations"
        ]
      }
    },
    "schematics": {
      "executor": "@rxap/plugin-library-schematics:build",
      "options": {
        "buildTarget": "firebase:build:production",
        "skipBuild": false,
        "tsConfig": "libs/firebase/tsconfig.schematics.json"
      }
    },
    "update-package-group": {
      "executor": "@rxap/plugin-library:update-package-group"
    },
    "publish": {
      "executor": "@rxap/plugin-library-publish:publish",
      "options": {
        "buildTarget": "firebase:build:production",
        "preTarget": "firebase:pack"
      }
    },
    "readme": {
      "executor": "@rxap/plugin-readme-generator:library"
    },
    "build-schematics": {
      "executor": "@rxap/plugin-library:build-schematics",
      "options": {
        "outputPath": "dist/libs/firebase",
        "tsConfig": "libs/firebase/tsconfig.schematics.json",
        "assets": [],
        "type": "schematics"
      }
    },
    "build-migrations": {
      "executor": "@rxap/plugin-library:build-schematics",
      "options": {
        "outputPath": "dist/libs/firebase",
        "tsConfig": "libs/firebase/tsconfig.migrations.json",
        "assets": [],
        "type": "migrations"
      }
    },
    "update-peer-dependencies": {
      "executor": "@rxap/plugin-library:update-peer-dependencies",
      "options": {
        "ignore": ["@firebase/auth", "@firebase/firestore", "jest-preset-angular", "ng-mocks", "@storybook\\/"]
      }
    }
  },
  "generators": {
    "@nrwl/angular:component": {
      "styleext": "scss"
    }
  },
  "tags": ["rxap"]
}
