{
  "name": "validator",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/validator/src",
  "prefix": "rxap",
  "targets": {
    "version": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["echo ok"]
      },
      "configurations": {
        "production": {}
      }
    },
    "build": {
      "executor": "@nx/angular:package",
      "options": {
        "project": "libs/validator/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/validator/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/validator/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["coverage/libs/validator"],
      "options": {
        "jestConfig": "libs/validator/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/validator/src/**/*.ts",
          "libs/validator/src/**/*.html"
        ]
      },
      "outputs": ["{options.outputFile}"]
    },
    "readme": {
      "executor": "@rxap/plugin-readme-generator:library"
    },
    "build-schematics": {
      "executor": "@rxap/plugin-library:build-schematics",
      "options": {
        "outputPath": "dist/libs/validator",
        "tsConfig": "libs/validator/tsconfig.schematics.json",
        "assets": [],
        "type": "schematics"
      }
    },
    "build-migrations": {
      "executor": "@rxap/plugin-library:build-schematics",
      "options": {
        "outputPath": "dist/libs/validator",
        "tsConfig": "libs/validator/tsconfig.migrations.json",
        "assets": [],
        "type": "migrations"
      }
    },
    "update-package-group": {
      "executor": "@rxap/plugin-library:update-package-group"
    },
    "update-peer-dependencies": {
      "executor": "@rxap/plugin-library:update-peer-dependencies",
      "options": {
        "ignore": []
      }
    }
  },
  "tags": []
}
