{
  "projectType": "library",
  "sourceRoot": "libs/socket-io/src",
  "prefix": "rxap",
  "targets": {
    "version": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["echo ok"]
      },
      "configurations": {
        "production": {}
      }
    },
    "build": {
      "executor": "@nrwl/angular:package",
      "options": {
        "project": "libs/socket-io/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/socket-io/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/socket-io/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/socket-io"],
      "options": {
        "jestConfig": "libs/socket-io/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/socket-io/src/**/*.ts",
          "libs/socket-io/src/**/*.html"
        ]
      },
      "outputs": ["{options.outputFile}"]
    },
    "pack": {
      "executor": "@rxap/plugin-pack:build",
      "options": {
        "targets": [
          "socket-io:build",
          "socket-io:build-schematics",
          "socket-io:build-migrations"
        ]
      },
      "configurations": {
        "production": {
          "targets": ["socket-io:build:production"]
        },
        "development": {
          "targets": ["socket-io:build:development"]
        }
      }
    },
    "readme": {
      "executor": "@rxap/plugin-readme-generator:library"
    },
    "build-schematics": {
      "executor": "@rxap/plugin-library:build-schematics",
      "options": {
        "outputPath": "dist/libs/socket-io",
        "tsConfig": "libs/socket-io/tsconfig.schematics.json",
        "assets": [],
        "type": "schematics"
      }
    },
    "build-migrations": {
      "executor": "@rxap/plugin-library:build-schematics",
      "options": {
        "outputPath": "dist/libs/socket-io",
        "tsConfig": "libs/socket-io/tsconfig.migrations.json",
        "assets": [],
        "type": "migrations"
      }
    },
    "update-package-group": {
      "executor": "@rxap/plugin-library:update-package-group"
    },
    "update-peer-dependencies": {
      "executor": "@rxap/plugin-library:update-peer-dependencies",
      "options": {
        "ignore": []
      }
    }
  },
  "tags": []
}
