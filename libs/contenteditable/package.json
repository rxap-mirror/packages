{
  "name": "@rxap/contenteditable",
  "version": "0.1.2",
  "peerDependencies": {
    "@rxap/utilities": ">=0.9.0",
    "rxjs": "^6.5.5",
    "@angular/core": "^10.0.6"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "schematics": "./schematics/collection.json",
  "ng-update": {
