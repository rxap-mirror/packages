{
  "name": "@rxap/contenteditable",
  "version": "11.0.0",
  "peerDependencies": {
    "@angular/core": "^11.0.5",
    "@rxap/utilities": ">=11.0.0",
    "rxjs": "^6.5.5"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "schematics": "./schematics/collection.json",
  "ng-update": {
    "migrations": "./schematics/migration.json",
    "packageGroup": [
      "@rxap/contenteditable",
      "@rxap/utilities"
    ]
  },
  "scripts": {
    "prepublish": "yarn --cwd ../../ nx run contenteditable:pack --with-deps",
    "version": "node ../../tools/update-dependencies.js $PWD",
    "preversion": "yarn --cwd ../../ nx run contenteditable:build --with-deps"
  },
  "publishConfig": {
    "directory": "../../dist/libs/contenteditable"
  }
}