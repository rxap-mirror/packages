{
  "name": "@rxap/table-system",
  "version": "10.0.1",
  "peerDependencies": {
    "@angular/animations": "^10.0.6",
    "@angular/cdk": "^10.1.1",
    "@angular/common": "^10.0.6",
    "@angular/core": "^10.0.6",
    "@angular/material": "^10.1.1",
    "@angular/platform-browser": "^10.0.6",
    "@rxap/utilities": ">=10.0.0",
    "rxjs": "^6.5.5"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "schematics": "./schematics/collection.json",
  "ng-update": {
    "migrations": "./schematics/migration.json",
    "packageGroup": [
      "@rxap/table-system",
      "@rxap/utilities"
    ]
  },
  "scripts": {
    "prepublish": "yarn --cwd ../../ nx run table-system:pack --with-deps",
    "version": "node ../../tools/update-dependencies.js $PWD",
    "preversion": "yarn --cwd ../../ nx run table-system:build --with-deps"
  },
  "publishConfig": {
    "directory": "../../dist/libs/table-system"
  }
}