{
  "name": "@rxap/table-system",
  "version": "0.0.3",
  "peerDependencies": {
    "@angular/common": ">=9",
    "@angular/core": ">=9",
    "@rxap/utilities": ">=0.3.19",
    "@angular/material": ">=9",
    "@angular/cdk": ">=9",
    "@angular/animations": ">=9",
    "@angular/platform-browser": ">=9",
    "rxjs": ">=6.5.5"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "schematics": "./schematics/collection.json",
  "ng-update": {
    "migrations": "./schematics/migration.json",
    "packageGroup": [
      "@rxap/table-system",
      "@rxap/utilities"
    ]
  },
  "scripts": {
    "prepublish": "yarn --cwd ../../ nx run table-system:pack --with-deps",
    "version": "node ../../tools/update-dependencies.js $PWD",
    "preversion": "yarn --cwd ../../ nx run table-system:build --with-deps"
  },
  "publishConfig": {
    "directory": "../../dist/libs/table-system"
  }
}